package am;

import java.util.*;

public class Sub01 {
	public static void run() {
		ArrayList<Car> cars=new ArrayList<>();
		
		Car car1=new Car("기아","K7","검정",3000);
		car1.setColor("흰색");
		car1.setDistance(1000000);
		car1.setDate("2021-03");
		cars.add(car1);
		for(Car car:cars) { //cars에서 car를 한대씩 가져와서 넣은것
			System.out.printf("%s\t%s\t%s\t%,dkm\t%s\n", //%s : 문자 \t : tab %d : 숫자
					car.getCompany(),car.getModel(),car.getColor(),car.getDistance(),car.getDate()); 
			System.out.println("");
		}
	}
}
