package juso;

import java.util.*;

public class Main {

	public static void main(String[] args) {
	      boolean run=true;
	      Scanner scan=new Scanner(System.in);
	      JusoDAO dao=new Juso;
	      while(run) {
	         System.out.println("------------------------------------------");
	         System.out.println("1.주소등록|2.주소조회|3.주소목록|4.주소삭제|0.종료");
	         System.out.println("------------------------------------------");
	         System.out.print("선택>");
	         String menu=scan.nextLine();
	         switch(menu) {
	         case "1":
	            break;
	         case "2":
	            break;
	         case "3": //주소목록
	        	 ArrayList<Juso> array=dao.list();
	            break;
	         case "4":   
	            break;
	         case "0":
	            run=false;
	            break;
	         default:
	            System.out.println("0-4메뉴를 선택하세요!");
	         }//switch
	      }//while
	      System.out.println("프로그램을 종료합니다.");

	}

}
