package am;

public class Student {
	//필드(class가 모두 가질 수 있는 속성)
	String no;
	String name;
	String juso;
	String phone;
	
	//생성자
	public Student() {
		
	}
	
	public Student(String no, String name) {
		this.no = no;
		this.name = name;
	}
	
	//메서드
	public void print() {
		System.out.printf("학번:%s\n이름:%s\n주소:%s\n전화번호:%s\n",no, name, juso, phone);
	}

}
